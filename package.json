{
  "name": "coqonect-web",
  "version": "1.0.0",
  "scripts": {
    "dev": "yarn tslint && next",
    "start": "yarn run run:githook && next start",
    "export": "next build && next export",
    "analyze": "ANALYZE=true next build",
    "build": "NODE_OPTIONS=\"--max_old_space_size=8000\" && next build",
    "type:check": "tsc",
    "run:githook": "git config core.hooksPath .githooks",
    "tslint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "build:tachyons": "tachyons src/styles/tachyons/tachyons.css > src/styles/tachyons.scss",
    "minify:tachyons": "tachyons src/styles/tachyons/tachyons.css --minify > src/styles/tachyons.min.scss"
  },
  "license": "ISC",
  "repository": "https://gitlab.com/hector101/coqonect-client.git",
  "author": "hector101 <hector1014really32@gmail.com>",
  "private": true,
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@apollo/react-ssr": "^3.1.3",
    "@material-ui/core": "^4.9.5",
    "@material-ui/icons": "^4.9.1",
    "@sweetalert/with-react": "^0.1.1",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link-http": "^1.5.16",
    "apollo-link-persisted-queries": "^0.2.2",
    "classnames": "^2.2.6",
    "cookie": "^0.4.0",
    "dotenv-webpack": "^1.7.0",
    "draft-js": "^0.11.4",
    "draft-js-export-html": "^1.4.1",
    "formik": "^2.0.4",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.15",
    "mobx": "^5.15.4",
    "mobx-react-lite": "^1.5.2",
    "next": "9.1.4",
    "next-compose-plugins": "^2.2.0",
    "next-with-apollo": "^4.3.0",
    "node-sass": "^4.13.0",
    "nprogress": "^0.2.0",
    "null-loader": "^3.0.0",
    "rc-dialog": "^7.5.14",
    "react": "^16.11.0",
    "react-apollo": "^3.1.3",
